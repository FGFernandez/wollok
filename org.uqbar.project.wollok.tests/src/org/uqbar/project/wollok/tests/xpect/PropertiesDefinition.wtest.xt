/* XPECT_SETUP org.uqbar.project.wollok.tests.xpect.WollokXPectTest END_SETUP */

describe "group of tests" {

	property var valor
	property const cuatro = 4
	var otroValor = self.valor() + 5
	var hello

	fixture {
		hello = "hola".length() + otroValor
	}
	
	test "cuatro es 4" {
		self.valor(4)
		assert.equals(self.valor(), self.cuatro())
		assert.equals(9, otroValor)
	}
	
	test "cuatro no es valor = 1" {
		// XPECT errors --> "Property are only allowed in objects, classes, describes and mixins" at "property"
		property var localVariable = 0
		// XPECT errors --> "Cannot modify constant property cuatro" at "cuatro"
		self.cuatro(1)
		assert.notEquals(self.valor(), cuatro)
	}
}
