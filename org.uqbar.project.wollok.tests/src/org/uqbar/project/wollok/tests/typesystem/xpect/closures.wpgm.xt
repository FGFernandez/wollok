/* XPECT_SETUP org.uqbar.project.wollok.tests.typesystem.xpect.TypeSystemXpectTestCase END_SETUP */

program p {
	var bool = true
	
	// XPECT type at c1 --> () => String
	const c1 = { "Hello" }

	// XPECT type at c2 --> (Number) => Number
	const c2 = { a => 2 + a }

	// XPECT type at c3 --> () => Void 
	const c3 = { }
	
	// XPECT type at c4 --> () => Boolean 
	const c4 = { return true }
	
	// XPECT type at c5 --> () => Integer
	const c5 = { if (bool) return 1 else return 2 }
	
	// XPECT type at c6 --> () => (Integer|String)
	const c6 = { 
		if (bool) { return 1 } 
		return "2"
	}
	
	// XPECT type at c7 --> () => (Integer|String)
	const c7 = { 
		if (bool) { return 1 } 
		"2"
	}
}
