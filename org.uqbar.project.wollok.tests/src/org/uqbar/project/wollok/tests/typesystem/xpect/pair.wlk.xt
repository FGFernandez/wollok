/* XPECT_SETUP org.uqbar.project.wollok.tests.typesystem.xpect.TypeSystemXpectTestCase END_SETUP */

object pairTest {
	
	// XPECT methodType at m1 --> () => Pair
	method m1() {
		return new Pair(2, 3)
	}
	
   	// XPECT methodType at m2 --> (Pair) => Any
	method m2(pair) {
		return pair.getKey()
	}

	// XPECT methodType at m3 --> (Pair) => Any
	method m3(pair) {
		return pair.getValue()
	}
	
	// This test is ignored until we have parametric types for Pair
   	// XPECT! methodType at m4 --> () => Number
	method m4() {
		return new Pair(2, 5).getKey()
	}
	
	// XPECT methodType at m5 --> (Number) => Pair
	method m5(n) {
		return new Pair(n + 1, n - 1)
	}
}
