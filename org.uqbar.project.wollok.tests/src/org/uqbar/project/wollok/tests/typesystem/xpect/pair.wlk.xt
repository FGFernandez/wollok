/* XPECT_SETUP org.uqbar.project.wollok.tests.typesystem.xpect.TypeSystemXpectTestCase END_SETUP */

object pairTest {
	
	// XPECT methodType at m1 --> () => Number
	method m1() {
		return new Pair(2, 3).getKey()
	}
	
   	// XPECT methodType at m2 --> (Pair) => Any
	method m2(pair) {
		return pair.getKey()
	}

	// XPECT methodType at m3 --> (Pair) => Any
	method m3(pair) {
		return pair.getValue()
	}
	
	// XPECT methodType at m4 --> (Number) => Pair
	method m4(n) {
		return new Pair(n + 1, n - 1)
	}

	// XPECT! methodType at m5 --> () => Number
	method m5() {
		return new Pair(2, 4).getKey()
	}
}
