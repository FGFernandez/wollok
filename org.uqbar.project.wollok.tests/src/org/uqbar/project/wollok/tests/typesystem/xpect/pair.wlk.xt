/* XPECT_SETUP org.uqbar.project.wollok.tests.typesystem.xpect.TypeSystemXpectTestCase END_SETUP */

object pairTest {
	
	// XPECT methodType at m1 --> () => Number
	method m1() {
		return new Pair(2, "hola").getKey()
	}
	
	// XPECT methodType at m2 --> () => String
	method m2() {
		return new Pair(2, "hola").getValue()
	}

   	// XPECT methodType at m3 --> (Pair) => Any
	method m3(pair) {
		return pair.getKey()
	}

	// XPECT methodType at m4 --> (Pair) => Any
	method m4(pair) {
		return pair.getValue()
	}
	
	// XPECT methodType at m5 --> (Number) => Pair
	method m5(n) {
		return new Pair(n + 1, n - 1)
	}

	method m6() {
		// XPECT type at n --> Number
		var n = new Pair(2, 3).getValue()

		// XPECT type at n --> String
		var s = new Pair(2, "").getValue()
	}

	// XPECT methodType at m7 --> (Number) => Number
	method m7(n) {
		/* XPECT warnings at p ---
			"Local variable used just to return. Consider removing it" at "p"
		--- */
		var p = new Pair(n + 1, n - 1)
		return p.getValue()
	}
}
